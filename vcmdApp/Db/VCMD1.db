record(bo,"$(P)Open")
	{
		field(DESC,"open")
		field(DTYP,"stream")
		field(OUT,"@VCMD1.proto Open $(PORT)")
		field(FLNK, "$(P)OpenCloseShutter.PROC CA")
	}
	
record(seq, "$(P)OpenCloseShutter")
	{
		field(DLY0,"2.0")
		field(DOL0,"0")
		field(LNK0, "$(P)Close PP MS")
		field(DLY1,"0.0")
		field(DOL1,"1")
		field(LNK1, "$(P)Open NPP MS")
	}

record(bo,"$(P)Close")
	{
		field(DESC,"close")
		field(DTYP,"stream")
		field(OUT,"@VCMD1.proto Close $(PORT)")
	}
  
